                "responses": {
                    "200": {
                        "description": "Offer updated - Example for sale",
                        "schema": {
                            "$ref": "#definitions/OfferSaleContext"
                        }
                    },
                    "404": {
                        "description": "not found",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    },
                    "500": {
                        "description": "Bad Subtype or Invalid offer",
                        "schema": {
                            "$ref": "#/definitions/Error"
